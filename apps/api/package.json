{
    "name": "@csv/api",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write 'src/**/*.ts'",
        "format:check": "prettier --check 'src/**/*.ts'",
        "type-check": "tsc --noEmit",
        "llm:crawler": "tsx src/services/llm.crawler.ts",
        "extraction:run": "tsx src/services/extraction.service.ts"
    },
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "@csv/types": "workspace:*"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.11.2",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^9.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}